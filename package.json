  "scripts": {

    "dev": "vue-cli-service serve --voxtype=shell",
    "build": "vue-cli-service build --target lib --name VoxApp --dest dist/shell ./src/shell/compile/dev.ts",

    "devengine": "vue-cli-service serve --voxtype=engineShell",
    "buildengine": "vue-cli-service build --target lib --name VoxApp --dest dist/shell ./src/engingShell/compile/dev.ts",

    "dev_engine_demoPrimitive": "vue-cli-service serve --voxtype=engine_demoPrimitive",
    "build_engine_demoPrimitive": "vue-cli-service build --target lib --name VoxApp --dest dist/demoPrimitive ./src/engine/demoPrimitive/compile/devApp.ts",

    "dev_renderer_playerOne": "vue-cli-service serve --voxtype=renderer_playerOne",
