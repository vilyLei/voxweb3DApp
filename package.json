    "core-js": "^3.3.2",
    "dat.gui": "^0.7.6",
    "dom-to-image": "^2.6.0",
    "draco3d": "http://116.62.211.118:8700/WeiChanghua/lt-draco3d.git",
    "encoding-japanese": "^1.0.30",
