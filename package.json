    "dev_modules_objDataParser": "vue-cli-service serve --voxtype=modules_objDataParser",
    "build_modules_objDataParser": "vue-cli-service build --target lib --name VoxApp --dest dist/modules/objDataParser ./src/modules/objDataParser/compile/devApp.ts"

