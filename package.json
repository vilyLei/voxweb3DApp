  "scripts": {

    "dev": "vue-cli-service serve --voxtype=shell",
    "build": "vue-cli-service build --target lib --name VoxApp --dest dist/shell ./src/shell/compile/dev.ts",

    "dev_playerOne": "vue-cli-service serve --voxtype=playerOne",
    "build_playerOne": "vue-cli-service build --target lib --name VoxApp --dest dist/playerOne ./src/playerOne/compile/devApp.ts",
    
    "dev_box": "vue-cli-service serve --voxtype=box",
    "build_box": "vue-cli-service build --target lib --name VoxApp --dest dist/box ./src/box/compile/devApp.ts",

    "dev_primitive": "vue-cli-service serve --voxtype=primitive",
    "build_primitive": "vue-cli-service build --target lib --name VoxApp --dest dist/primitive ./src/primitive/compile/devApp.ts",

    "dev_oscillator": "vue-cli-service serve --voxtype=oscillator",
    "build_oscillator": "vue-cli-service build --target lib --name VoxApp --dest dist/oscillator ./src/oscillator/compile/devApp.ts",

    "dev_mouseEvent": "vue-cli-service serve --voxtype=mouseEvent",
    "build_mouseEvent": "vue-cli-service build --target lib --name VoxApp --dest dist/mouseEvent ./src/mouseEvent/compile/devApp.ts",
  
    "dev_fps": "vue-cli-service serve --voxtype=fps",
    "build_fps": "vue-cli-service build --target lib --name VoxApp --dest dist/fps ./src/fps/compile/devApp.ts"

  },
